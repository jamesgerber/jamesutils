ISOlist={'BRN',...
'KHM',...
'IDN',...
'LAO',...
'MMR',...
'MYS',...
'PHL',...
'SGP',...
'THA',...
'TLS',...
'VNM'};

%CountryNames=
g0=getgeo41_g0;

ii=datablank

for j=1:numel(ISOlist);
    idx=strcmp(ISOlist{j},g0.gadm0codes);
    CountryNameList{j}=g0.namelist0{idx};
end

if nargin==1
    ISOlist=ISOlist{m};
    CountryNameList=CountryNameList{m};
end

